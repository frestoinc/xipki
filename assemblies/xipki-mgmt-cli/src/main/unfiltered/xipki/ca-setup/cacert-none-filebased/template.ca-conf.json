{
	"signers":[],
	"keypairGens":[],
	"requestors":[{
		// id 1 is reserved
		"id":2,
		"name":"requestor1",
		"type":"cert",
		"binaryConf":{
			"file":"keycerts/ra-sdk-client-cert.pem"
		}
	}],
	"publishers":[
		/*
		{
			"id":1
			"name":"ocsp-publisher",
			"type":"ocsp",
			"conf":{
				"value":"datasource=ocsp,publish.goodcerts=true"
			}
		}
		*/
	],
	"profiles":[{
		"id":1,
		"name":"rootca",
		"type":"xijson",
		"conf":{
			"file":"certprofile/certprofile-rootca.json"
		}
	},{
		"id":2,
		"name":"scep",
		"type":"xijson",
		"conf":{
			"file":"certprofile/certprofile-scep.json"
		}
	},{
		"id":3,
		"name":"ocsp",
		"type":"xijson",
		"conf":{
			"file":"certprofile/certprofile-ocsp.json"
		}
	},{
		"id":4,
		"name":"smime",
		"type":"xijson",
		"conf":{
			"file":"certprofile/certprofile-smime.json"
		}
	},{
		"id":5,
		"name":"tls",
		"type":"xijson",
		"conf":{
			"file":"certprofile/certprofile-tls.json"
		}
	},{
		"id":6,
		"name":"tls-c",
		"type":"xijson",
		"conf":{
			"file":"certprofile/certprofile-tls-c.json"
		}
	},{
		"id":7,
		"name":"tls-sm2",
		"type":"xijson",
		"conf":{
			"file":"certprofile/certprofile-tls-sm2.json"
		}
	},{
		"id":8,
		"name":"tls-sm2-enc",
		"type":"xijson",
		"conf":{
			"file":"certprofile/certprofile-tls-sm2-enc.json"
		}
	}],
	"cas":[{
		"id":1,
		"name":"$[ca.name]",
		"aliases":[
			"$[ca.alias]"
		],
		"caInfo":{
			"caUris":{
				"crlUris":[
					"http://ca.myorg.org/crl/$[ca.name]"
				],
				"ocspUris":[
					"http://localhost:8080/ocsp/"
				]
			},
			"keypairGenNames": ["software"],
			"crlControl":{
				"interval.hours": 24,
				"fullcrl.intervals":7,
				"overlap":"3d",
				"interval.time":"02:00"
			},
			"expirationPeriod":365,
			"genSelfIssued":{
				"subject":"REPLACEME-SUBJECT",
				"profile":"rootca",
				"serialNumber":"1"
			},
			"keepExpiredCertDays":-1,
			"maxValidity":"10y",
			"nextCrlNo":2,
			"permissions":["all"],
			"saveCert":true,
			"saveKeypair":false,
			"signerType":"REPLACEME-TOKEN_TYPE",
			"signerConf":{
				"value":"REPLACEME-CA_SIGNERCONF"
			},
			"snSize":20,
			"status":"active",
			"validityMode":"cutoff"
		},
		"profiles":["ocsp", "scep", "smime", "tls", "tls-c", "tls-sm2", "tls-sm2-enc"
		],
		"publishers":[
			// "ocsp-publisher"
		],
		"requestors":[{
			"requestorName":"requestor1",
			"permissions":["all"],
			"profiles":["all"]
		}]
	}]
}

