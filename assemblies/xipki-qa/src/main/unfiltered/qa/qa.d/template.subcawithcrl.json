{
	"basedir":"APP_DIR",
	"properties":{
		"token.type":"REPLACEME-TOKEN_TYPE",
		"ca.signerConf": "REPLACEME-CA_SIGNERCONF"
	},
	"cas":[{
		"name":"subcawithcrl1",
		"aliases":[
			"subcawithcrl"
		],
		"caInfo":{
			"caUris":{
				"cacertUris":[
					"http://myorg.org/subcawithcrl1.der"
				],
				"crlUris":[
					"http://localhost:8081/crl/?type=crl&name=subcawithcrl1"
				],
				"deltaCrlUris":[
					"http://localhost:8081/crl/?type=deltacrl&name=subcawithcrl1"
				],
				"ocspUris":[
					"http://localhost:8080/ocsp/responder2"
				]
			},
			"cert":{
				"file":"output/subcawithcrl1.crt"
			},
			"certchain":[{
				"file":"output/rootca1.crt"
			}],			
			"crlControl":{
				"interval.hours": 24,
				"fullcrl.intervals":7,
				"overlap":"3d",
				"interval.time":"02:00"
			},
			"keypairGenNames":["software"],
			"expirationPeriod":365,
			"keepExpiredCertDays":-1,
			"maxValidity":"5y",
			"nextCrlNo":2,
			"permissions":["all"],
			"saveCert":true,
			"saveKeypair":false,
			"signerType":"${token.type}",
			"signerConf":{
				"value":"${ca.signerConf}"
			},
			"snSize":20,
			"status":"active",
			"validityMode":"lax"
		},
		"profiles":[
			"ocsp",
			"scep",
			"multiple-ous",
			"multi-valued-rdn",
			"max-time",
			"tls", "tls-enc",
			"gmt0015",
			"tls-sm2", "tls-sm2-enc"
		],
		"requestors":[{
			"requestorName":"requestor1",
			"permissions":[
				"all"
			],
			"profiles":[
				"all"
			]
		}]
	}]
}

