{
	 "basedir":"APP_DIR",
	"properties":{
		"token.type":"REPLACEME-TOKEN_TYPE",
		"ca.signerConf": "REPLACEME-CA_SIGNERCONF"
	},
	"cas":[{
		"name":"subca1",
		"aliases":[
			"subca"
		],
		"caInfo":{
			"caUris":{
				"cacertUris":[
					"http://myorg.org/subca1.der"
				],
				"crlUris":[
					"http://localhost:8081/crl/?type=crl&name=subca1"
				],
				"ocspUris":[
					"http://localhost:8080/ocsp/responder1"
				]
			},
			"cert":{
				"file":"output/subca1.crt"
			},
			"certchain":[{
				"file":"output/rootca1.crt"
			}],
			"kceypairGenNames":["software"],
			"ctlogControl":{
				"enabled":true,
				"servers":"https://localhost:8444/ctlog/ctlogrsa;https://localhost:8444/ctlog/ctlogec",
				"sslcontext.name":"ctlog1"
			},
			"expirationPeriod":365,
			"revokeSuspendedControl":{
				"enabled":true,
				"targetReason":"cessationOfOperation",
				"unchangedSince":"15d"
			},
			"keepExpiredCertDays":-1,
			"maxValidity":"5y",
			"nextCrlNo":2,
			"permissions":["all"],
			"saveCert":true,
			"saveKeypair":false,
			"signerType":"${token.type}",
			"signerConf":{
				"value":"${ca.signerConf}"
			},
			"snSize":20,
			"status":"active",
			"validityMode":"strict"
		},
		"profiles":[
			"cab-domain-validated",
			"cab-org-validated",
			"cab-individual-validated"
		],
		"publishers":[
			"ocsp-publisher"
		],
		"requestors":[{
			"requestorName":"requestor1",
			"permissions":["all"],
			"profiles":["all"]
		}]
	}]
}

